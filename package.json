{
  "name": "sdfjs",
  "version": "0.1.0",
  "description": "A javascript library to help read zip files in Standard Deck Format (SDF)",
  "main": "index.js",
  "scripts": {
    "build": "browserify index.ts --s sdf -p [ tsify --noImplicitAny ] > sdf.js"
  },
  "author": "spacemandev",
  "license": "GPLv3",
  "dependencies": {
    "browserify": "^16.5.1",
    "handlebars": "^4.7.6",
    "js-yaml": "^3.13.1",
    "jszip": "^3.7.0",
    "tsify": "^4.0.1",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/handlebars": "^4.1.0",
    "@types/js-yaml": "^3.12.3",
    "@types/jszip": "^3.1.7"
  }
}
